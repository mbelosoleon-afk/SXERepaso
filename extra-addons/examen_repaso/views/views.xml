<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="examen_repaso.form">
      <field name="name">examen_repaso.examen_repaso form</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="base.view_partner_form"/>
      <field name="arch" type="xml">
        <xpath expr="//notebook" position="inside">
          <page string="PROYECTO">
            <group>
              <field name="nombre" string="Nombre"/>
              <field name="apellidos" string="Apellidos"/>
              <field name="proyecto" string="Descripción del Proyecto"/>
              <field name="tutor" string="Tutor Asignado"/>
              <field name="horasDia" string="Horas al día"/>
              <field name="horasMes" string="Horas al mes"/>
            </group>
          </page>
        </xpath>
      </field>
    </record>



